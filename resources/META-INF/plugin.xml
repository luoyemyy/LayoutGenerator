<idea-plugin>
  <id>com.com.ao.layout.generator</id>
  <name>layout generator</name>
  <version>1.0</version>
  <vendor email="luoyemyy@qq.com" url="https://github.com/luoyemyy/LayoutGenerator">Github:luoyemyy</vendor>

  <description><![CDATA[
     通过查找布局文件中的有id的view，自动生成findViewById 和 onClick
    ]]></description>

  <change-notes><![CDATA[

    ]]>
  </change-notes>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
